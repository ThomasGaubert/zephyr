    <string name="menu_privacy_uuid_title">Instance ID</string>
    <string name="menu_privacy_uuid_text">Zephyr uses a randomly generated ID to track crashes and usage data. This ID is generated on a fresh install and isn\'t linked to you or other Zephyr clients.</string>
