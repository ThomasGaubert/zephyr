    <!-- Privacy Menu -->
    <string name="menu_privacy_title">Privacy</string>
    <string name="menu_privacy_usage_text">Zephyr collects anonymous usage data and crash reports to improve your experience. Notifications never leave your local network.</string>
    <string name="menu_privacy_setting_crash_reports">Crash reports</string>
