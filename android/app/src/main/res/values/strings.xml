    <!-- What's New Menu -->
    <string name="menu_whats_new_zephyr_v2_title">Welcome to Zephyr 2.0!</string>
    <string name="menu_whats_new_zephyr_v2_body">Every aspect of Zephyr has been completely rewritten to address common feedback, improve performance and stability, and to make future development easier.</string>
    <string name="menu_whats_new_steam_title">Steam</string>
    <string name="menu_whats_new_steam_body">The Zephyr desktop client is now available on Steam.\n\nNote that the old client is no longer supported.</string>
    <string name="menu_whats_new_steam_button_text">Download on Steam</string>
    <string name="menu_whats_new_connect_title">Quick connect</string>
    <string name="menu_whats_new_connect_body">Zephyr will automatically find any servers running on your local network.\n\nYou can also scan a QR code or enter a code to get connected quickly.</string>
    <string name="menu_whats_new_notif_title">Enhanced Notifications</string>
    <string name="menu_whats_new_notif_body">Notification icons are now displayed in VR!\n\nZephyr also lists previous notifications on the desktop and within VR.</string>

